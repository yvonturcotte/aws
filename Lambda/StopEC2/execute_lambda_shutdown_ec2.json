{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Stmt1453628436000",
            "Effect": "Allow",
            "Action": [
                "ec2:Describe*",
                "ec2:StopInstances"
            ],
            "Resource": [
                "*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": "arn:aws:logs:*:*:*"
        }
    ]
}